{
  "name": "theme-bundler",
  "version": "1.0.0",
  "description": "theme bundler",
  "main": "webpack.config.js",
  "scripts": {
    "a11y:url": "npx pa11y --runner \"axe\" --runner \"htmlcs\" \"$@\"",
    "browserslist:update": "yes \"y\" | npx browserslist@latest --update-db && npx browserslist",
    "build": "npm run \"dist:clean\" && npm run \"format\" && npm run \"browserslist:update\" && npx webpack --mode=\"production\"",
    "build:prod": "npm run \"build\"",
    "build:prod:fix": "npm run \"format:fix\" && npm run \"lint:fix\" && npm run \"build\"",
    "build:dev": "npm run \"dist:clean\" && npm run \"format\" && npx webpack --mode=\"development\"",
    "deps:constrain": "cat \"webpack/deps-constrain.txt\" | xargs npm install --save-dev",
    "deps:fix": "rm -v package.json && rm -v package-lock.json && rm -rfv node_modules && git fetch && git checkout origin/master -- ./package.json && git checkout origin/master -- ./package-lock.json",
    "deps:install": "rm -rfv node_modules && npm install && npm run \"browserslist:update\" && npm run \"deps:update\" && npm run \"deps:constrain\" && npm audit --omit=dev",
    "deps:update": "npm outdated | awk 'NR>1 {print $1\"@\"$4}' | xargs npm install && npm audit --omit=dev",
    "dist:clean": "rm -rfv \"../dist\"",
    "format:css": "npx prettier --check \"css/**/*.css\"",
    "format:css:fix": "npx prettier --write \"css/**/*.css\"",
    "format:js": "npx prettier --check \"./**/*.js\"",
    "format:js:fix": "npx prettier --write \"./**/*.js\"",
    "format": "npm run \"format:css\" && npm run \"format:js\"",
    "format:fix": "npm run \"format:css:fix\" && npm run \"format:js:fix\"",
    "lint": "npm run \"lint:css\" && npm run \"lint:js\"",
    "lint:css": "npx stylelint \"css/**/*.css\"",
    "lint:css:fix": "npx stylelint --fix \"css/**/*.css\"",
    "lint:fix": "npm run \"lint:css:fix\" && npm run \"lint:js:fix\"",
    "lint:js": "npx eslint \"./**/*.js\"",
    "lint:js:fix": "npx eslint --fix \"./**/*.js\"",
    "node:version:lock": "touch \".nvmrc\" && node -v > .nvmrc",
    "serve": "npm run \"dist:clean\" && npm run \"browserslist:update\" && npx webpack --env SERVE --mode=\"development\" --watch",
    "start": "npm run \"watch\"",
    "start:dev": "npm run \"watch\"",
    "watch": "npm run \"dist:clean\" && npx webpack --mode=\"development\" --watch"
  },
  "keywords": [
    "bundler",
    "webpack",
    "postcss",
    "es6",
    "jsx",
    "babel"
  ],
  "author": "Birk Thestrup Blauner",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv": "^16.0.1",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^2.6.1",
    "pa11y": "^6.2.3",
    "postcss": "^8.4.16",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-functions": "^4.0.2",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.0",
    "postcss-mixins": "^9.0.3",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.8.0",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.7.1",
    "slash": "^4.0.0",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "svg-sprite-loader": "^6.0.11",
    "svg-url-loader": "^7.1.1",
    "svgo": "^2.8.0",
    "svgo-loader": "^3.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
